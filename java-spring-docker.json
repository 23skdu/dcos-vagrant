{
    "id": "/java-spring-docker",
    "cpus": 0.25,
    "mem": 275,
    "instances": 1,
    "ports": [0],
    "labels": {
        "HAPROXY_GROUP": "external",
        "HAPROXY_0_VHOST": "spring.acme.org"
    },
    "constraints": [
        [
            "hostname",
            "UNIQUE"
        ]
    ],
    "healthChecks": [
        {
            "protocol": "HTTP",
            "portIndex": 0,
            "path": "/",
            "gracePeriodSeconds": 10,
            "intervalSeconds": 10,
            "maxConsecutiveFailures": 2,
            "timeoutSeconds": 30
        }
    ],
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "stathyinc/java-spring:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 3060,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    }
}
