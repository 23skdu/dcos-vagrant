{
    "id": "/spring-test",
    "cpus": 0.25,
    "mem": 384,
    "instances": 3,
    "cmd": "jre*/bin/java -Xmx256m -jar gs-spring-boot-0.1.0.jar --server.port=$PORT0",
    "constraints": [
        [
            "hostname",
            "UNIQUE"
        ]
    ],
    "ports": [0],
    "healthChecks": [
        {
            "protocol": "HTTP",
            "portIndex": 0,
            "path": "/",
            "gracePeriodSeconds": 5,
            "intervalSeconds": 20,
            "maxConsecutiveFailures": 3
        }
    ],
    "uris": [
        "https://downloads.mesosphere.com/demo/java/gs-spring-boot-0.1.0.jar",
        "https://downloads.mesosphere.com/java/jre-8u51-linux-x64.tar.gz"
    ]
}

